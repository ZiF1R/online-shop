{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "components/*": ["components/*"],
      "services/*": ["services/*"],
      "stores/*": ["stores/*"],
      "types/*": ["types/*"],
      "views/*": ["views/*"],
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
